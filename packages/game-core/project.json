{
  "name": "game-core",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/game-core/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/game-core",
        "main": "packages/game-core/src/index.ts",
        "tsConfig": "packages/game-core/tsconfig.lib.json",
        "assets": ["packages/game-core/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/game-core/**/*.ts"]
      }
    }
  },
  "tags": []
}
