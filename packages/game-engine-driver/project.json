{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/game-engine-driver/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/game-engine-driver",
        "main": "packages/game-engine-driver/src/index.ts",
        "tsConfig": "packages/game-engine-driver/tsconfig.lib.json",
        "assets": ["packages/game-engine-driver/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/game-engine-driver/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/game-engine-driver"],
      "options": {
        "jestConfig": "packages/game-engine-driver/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
